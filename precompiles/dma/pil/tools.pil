
function get_continuation_id(const int base_cont_id, const int max_cont): int {
    container proof.continuation.`${AIRTEMPLATE}` alias continuation {
        int id = base_cont_id;
    }
    const int id = continuation.id;
    assert(id < base_cont_id);
    continuation.id += continuation.id;
    return id;
}

